<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@foreach (var job in ViewBag.Jobs)
{
    <table class="job-listing">
        <tbody>
            @foreach (var jobField in job)
             {
                <tr>
                    <td>@jobField.Key</td>
                    <td>@jobField.Value</td>
                </tr>
             }
           
        </tbody>
    </table>

}
